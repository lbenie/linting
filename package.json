{
  "name": "@lbenie/linting",
  "version": "1.3.3",
  "description": "my opiniated rules",
  "repository": {
    "url": "https://github.com/lbenie/linting"
  },
  "keywords": [
    "eslint",
    "stylelint",
    "linting"
  ],
  "type": "commonjs",
  "license": "MIT",
  "private": false,
  "files": [
    "eslint",
    "stylelint"
  ],
  "main": "./eslint/.eslintrc.js",
  "exports": {
    "./eslint": "./eslint/.eslintrc.js",
    "./stylelint": "./stylelint/.stylelintrc.js"
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "test": "concurrently -n vitest 'vitest --coverage'",
    "test:vitest": "vitest",
    "lint": "concurrently -n eslint, stylelint 'eslint src' 'stylelint src/**/*.styl'"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "10.0.4",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "9.0.4",
    "@semantic-release/npm": "10.0.5",
    "@semantic-release/release-notes-generator": "11.0.7",
    "@types/react": "18.2.21",
    "@types/semantic-release": "20.0.1",
    "all-contributors-cli": "6.26.1",
    "concurrently": "8.2.1",
    "conventional-changelog-conventionalcommits": "7.0.1",
    "lit": "2.8.0",
    "react": "18.2.0",
    "semantic-release": "21.1.1",
    "tsutils": "3.21.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "eslint": "8.48.0",
    "eslint-plugin-astro": "^0.29.0",
    "eslint-plugin-functional": "6.0.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-lit-a11y": "4.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "postcss": "8.4.29",
    "postcss-scss": "4.0.7",
    "postcss-styl": "0.12.3",
    "prettier": "3.0.3",
    "stylelint": "15.10.3",
    "stylelint-order": "6.0.3",
    "stylelint-stylus": "0.18.0"
  }
}
